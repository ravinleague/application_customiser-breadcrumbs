{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "breadcrumbs-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/breadcrumbs/BreadcrumbsApplicationCustomizer.js",
          "manifest": "./src/extensions/breadcrumbs/BreadcrumbsApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-init": {
      "path": "https://jellypod.sharepoint.com/_layouts/15/init.js",
      "globalName": "$_global_init"
    },
    "microsoft-ajax": {
      "path": "https://jellypod.sharepoint.com/_layouts/15/MicrosoftAjax.js",
      "globalName": "Sys",
      "globalDependencies": [
        "sp-init"
      ]
    },
    "sp-runtime": {
      "path": "https://jellypod.sharepoint.com/_layouts/15/SP.Runtime.js",
      "globalName": "SP",
      "globalDependencies": [
        "microsoft-ajax"
      ]
    },
    "sharepoint": {
      "path": "https://jellypod.sharepoint.com/_layouts/15/SP.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    },
    "sp-taxonomy": {
      "path": "https://jellypod.sharepoint.com/_layouts/15/SP.Taxonomy.js",
      "globalName": "SP",
      "globalDependencies": [
        "sharepoint"
      ]
    },
    "breadcrumb-utils": {
      "path": "https://jellypod.sharepoint.com/testnav/SiteAssets/utilities/BreadcrumbsUtils.js",
      "globalName": "BreadcrumbUtils"
    }
  },
  "localizedResources": {
    "BreadcrumbsApplicationCustomizerStrings": "lib/extensions/breadcrumbs/loc/{locale}.js"
  }
}
